{
  "name": "ai-prompt-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=v18.20.3",
    "npm": ">=10.5.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepare": "husky || true && npm run husky",
    "husky": "echo 'npx lint-staged' > .husky/pre-commit && echo 'npx --no-install commitlint --edit $1' > .husky/commit-msg",
    "lint": "eslint . --ext js,jsx,ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "prettier . --write --check --fix",
    "style": "npx stylelint --fix \"src/**/*.(scss|css)\"",
    "tailwindcss": "npx tailwindcss -i ./src/main.scss -o ./public/tailwind.css --watch && npx tailwindcss -o ./public/tailwind.css --minify"
  },
  "dependencies": {
    "@ant-design/pro-components": "^2.6.51",
    "@uiw/react-md-editor": "^4.0.4",
    "antd": "^5.15.3",
    "file-saver": "^2.0.5",
    "normalize.css": "^8.0.1",
    "query-string": "^9.0.0",
    "react": "^18.2.0",
    "react-custom-scrollbars-2": "^4.5.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.22.3",
    "rehype-sanitize": "^6.0.0"
  },
  "devDependencies": {
    "@ant-design/cssinjs": "^1.20.0",
    "@ant-design/icons": "^5.3.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@dnd-kit/core": "^6.1.0",
    "@dnd-kit/modifiers": "^7.0.0",
    "@dnd-kit/sortable": "^8.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@emotion/react": "^11.11.4",
    "@ianvs/prettier-plugin-sort-imports": "^4.2.1",
    "@microlink/react-json-view": "^1.23.0",
    "@tailwindcss/aspect-ratio": "^0.4.2",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.13",
    "@types/file-saver": "^2.0.7",
    "@types/lodash": "^4.17.0",
    "@types/mockjs": "^1.0.10",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "@vitejs/plugin-react": "^4.2.1",
    "antd-style": "^3.6.2",
    "autoprefixer": "^10.4.18",
    "classnames": "^2.5.1",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "lodash": "^4.17.21",
    "mockjs": "^1.1.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.0.1",
    "postcss-nesting": "^12.1.0",
    "prettier": "^3.3.0",
    "sass": "^1.72.0",
    "stylelint": "^16.6.1",
    "stylelint-config-prettier": "^9.0.5",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-standard": "^36.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.0",
    "tailwindcss": "^3.4.1",
    "terser": "^5.31.1",
    "typescript": "^5.2.2",
    "uuid": "^9.0.1",
    "vanilla-jsoneditor": "^0.23.0",
    "vite": "^5.1.6",
    "zustand": "^4.5.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "npm run prettier",
      "npm run lint",
      "npm run style"
    ]
  },
  "main": "index.js",
  "repository": "https://github.com/Alpha-CL/ai-prompt-front.git",
  "author": "Alpha-CL <Liang1992@12>",
  "license": "MIT"
}
